
let string = prompt("Digite uma string:");


let caracteres = string.split("");


let caracteresReversos = [];
for (let i = caracteres.length - 1; i >= 0; i--) {
  caracteresReversos.push(caracteres[i]);
}


let novaString = caracteresReversos.join("");


console.log("A string invertida Ã©:", novaString);
